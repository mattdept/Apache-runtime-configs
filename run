echo;echo === Current runtime Apache configurations on `hostname` for Apache Parent PID: `pgrep -u root httpd` ===;echo ;for i in "server_limit" "ap_daemons_to_start" "min_spare_threads" "max_spare_threads" "threads_per_child" "max_workers"  "thread_limit"; do  gdb --batch --quiet -ex "print $i" -ex "quit" -p `pgrep -u root httpd`|grep '$1' |sed "s/\$1/$i/g" ;done |sed 's/threads_per_child/ThreadsPerChild/' |sed 's/ap_daemons_to_start/StartServers/' |sed 's/min_spare_threads/MinSpareThreads/' |sed 's/max_spare_threads/MaxSpareThreads/' |sed 's/max_workers/MaxRequestWorkers/' |sed 's/server_limit/ServerLimit/' |sed 's/thread_limit/ThreadLimit/';echo
